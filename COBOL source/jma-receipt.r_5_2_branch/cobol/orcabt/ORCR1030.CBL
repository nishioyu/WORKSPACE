      *
           MOVE    ZERO                 TO  IDX-RECE130
           MOVE    SPACE                TO  WRK-RECE130XX-REC
           INITIALIZE                       WRK-RECE130XX-REC
      *
           PERFORM
               UNTIL  ( FLG-RECE130           =   1              ) OR
                      ( RECE120-KEY1      NOT =   RECE130-KEY1   ) OR
                      ( RECE120-ZAINUM    NOT =   RECE130-ZAINUM )
      *
               PERFORM   VARYING   IDX   FROM    1   BY  1
                         UNTIL   ( IDX               >   5     )
                         OR      ( RECE130-SRYCD(IDX) =   SPACE )
                 ADD    1             TO      IDX-RECE130
                 MOVE   RECE130-SRYACT-TBL(IDX)
                       TO   WRK-RECE130XX-SRYACT-TBL(IDX-RECE130 1)
      *++++++
                 IF    IDX-RECE130  =  RECE120-MEISAISU
      *
                   IF  WRK-RECE130XX-SRYCD(IDX-RECE130 1)(1:1)
                                                = "8"
                   OR  WRK-RECE130XX-SRYCD(IDX-RECE130 1)(1:3)
                                                = "008"
      *
                     COMPUTE   IDV  =
                      (WRK-RECE130XX-YUKOKETA(IDX-RECE130 1) * 2) + 1
      *
                     MOVE    1    TO   IDV2
                     IF  WRK-RECE130XX-INPUTCOMENT(IDX-RECE130 1)
                                                        NOT = SPACE
                       PERFORM  VARYING  IDV3  FROM  140  BY  -1
                                 UNTIL   IDV3    <     1
                         IF  WRK-RECE130XX-INPUTCOMENT(IDX-RECE130 1)
                                              (IDV3:1)  NOT = SPACE
                           COMPUTE  IDV2 = IDV2 + IDV3
                           MOVE     1   TO IDV3
                         END-IF
                       END-PERFORM
                     END-IF
      *
                     IF   IDV2  <= 131
                       MOVE    "　×"   TO
                       WRK-RECE130XX-INPUTCOMENT(IDX-RECE130 1)(IDV2:4)
                     END-IF
                     IF   IDV   <= 291
                       MOVE    "　×"   TO
                       WRK-RECE130XX-NAME(IDX-RECE130 1)(IDV:4)
                     END-IF
      *
                     COMPUTE   IDV  =  IDV  +  4
                     COMPUTE   IDV2 =  IDV2 +  4
      *
                     MOVE    RECE120-ZAIKAISU   TO   DAY-IDX4
                     PERFORM 20011-KAISU-HENSYU2-SEC
                     IF   IDV2  <= 135
                       MOVE    WRK-HENSYU-KAISU4  TO
                       WRK-RECE130XX-INPUTCOMENT(IDX-RECE130 1)(IDV2:6)
                     END-IF
                     IF   IDV   <= 295
                       MOVE    WRK-HENSYU-KAISU4  TO
                       WRK-RECE130XX-NAME(IDX-RECE130 1)(IDV:6)
                       COMPUTE WRK-RECE130XX-YUKOKETA(IDX-RECE130 1) =
                               WRK-RECE130XX-YUKOKETA(IDX-RECE130 1) +
                               5
                     END-IF
      *
