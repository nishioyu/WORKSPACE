       270-SENTAKUCOM-HENSYU-EXT.
           EXIT.
      *
      *****************************************************************
      *    選択式コメント編集２
      *****************************************************************
       2701-SENTAKUCOM-HENSYU2-SEC     SECTION.
      *
           MOVE    ZERO                TO  IDX45
           MOVE    SPACE               TO  WRK-RECE130XX-REC
           INITIALIZE                      WRK-RECE130XX-REC
      *
           MOVE    ZERO                TO  FLG-SENTAKU-SYUGI
           INITIALIZE                      TBL-GAZO-INF
      *
           PERFORM 800-RECE1201-SET1301-SEC
           MOVE    RECE1201-ZAINUM     TO  RECE1301-ZAINUM
           START   RECE1301-FILE
               KEY IS    >=   RECE1301-KEY
               INVALID
                   MOVE    1           TO  FLG-RECE1301
               NOT INVALID
                   PERFORM 990-RECE1301-NEXT-SEC
           END-START
           PERFORM
               UNTIL  ( FLG-RECE1301          =   1               )
               OR     ( RECE1201-KEY1     NOT =   RECE1301-KEY1   )
               OR     ( RECE1201-ZAINUM   NOT =   RECE1301-ZAINUM )
      *
               PERFORM  VARYING  IDX  FROM  1  BY  1
                         UNTIL  (IDX                 >   5  )
                          OR    (RECE1301-SRYCD(IDX) = SPACE)
                 ADD    1   TO   IDX45
                 MOVE   RECE1301-SRYACT-TBL(IDX)
                            TO   WRK-RECE130XX-SRYACT-TBL(IDX45 1)
      *
                 IF    (RECE1201-FLG-CTMRI-COMMENT  =   SPACE)
                 AND   (RECE1201-SENTAKUBUIFLG  NOT =   SPACE)
                   IF   WRK-RECE130XX-SRYCD(IDX45 1)(1:1) = "1"
                         MOVE    1    TO  FLG-SENTAKU-SYUGI
                   END-IF
                   IF   WRK-RECE130XX-SRYCD(IDX45 1)(1:3) = "002"
                     MOVE  "1"  TO  WRK-RECE130XX-RECEPTFLG(IDX45 1)
                     MOVE   1   TO  FLG-SENTAKU-OKIKAE
                   END-IF
                   IF  ((WRK-RECE130XX-SRYCD(IDX45 1)(1:1) = "8"   ) AND
                        (WRK-RECE130XX-SRYCD(IDX45 1)(4:1) = "1"   ) AND
                        (WRK-RECE130XX-SRYCD(IDX45 1)(5:1) = "8"   ))
                   OR  ( WRK-RECE130XX-SRYCD(IDX45 1) = "830100797" )
                     MOVE     ZERO  TO  FLG-GAZO
                     PERFORM  VARYING  IDX50  FROM  1  BY  1
                               UNTIL  (IDX50 >   10                  )
                                OR    (IDX50 > RECE1201-SENTAKUBUICNT)
                                OR    (WRK-RECE130XX-NAME(IDX50 1)
                                                              = SPACE)
                       IF   (WRK-RECE130XX-NAME(IDX45 1) =
                                       WRK-RECE130XX-NAME(IDX50 1))
                       AND  (TBL-GAZO-CHEKFLG(IDX50)     =  ZERO  )
                         MOVE   1   TO  FLG-GAZO
                         MOVE   10  TO  IDX50
                       END-IF
                     END-PERFORM
                     IF  FLG-GAZO  =  ZERO
                     AND IDX50    <=  10
                       MOVE   WRK-RECE130XX-SRYACT-TBL(IDX45 1)
                                  TO  WRK-RECE130XX-SRYACT-TBL(IDX50 1)
                       IF     FLG-SENTAKU-SYUGI  =  ZERO
                         MOVE 1   TO  TBL-GAZO-CHEKFLG(IDX50)
                       END-IF
                     END-IF
                     MOVE    "1"  TO  WRK-RECE130XX-RECEPTFLG(IDX45 1)
                     MOVE     1   TO  FLG-SENTAKU-OKIKAE
                   END-IF
                 END-IF
      *
                 IF  (IDX45                                >  1        )
                 AND (WRK-RECE130XX-SRYCD(IDX45 1)(1:1)    = "8"       )
                 AND (WRK-RECE130XX-SRYCD(IDX45 1)(4:1)    = "1" OR "6")
                 AND (WRK-RECE130XX-MEISKYFLG(IDX45 1) NOT = "2"       )
                   MOVE      ZERO    TO   FLG-GET
                   MOVE      ZERO    TO   FLG-ETC
                   COMPUTE   IDY12   =    IDX45  -  1
                   PERFORM  VARYING  IDY11  FROM  IDY12  BY  -1
                             UNTIL  (IDY11    <  1)
                              OR    (FLG-GET  =  1)
                     IF (WRK-RECE130XX-SRYCD(IDY11 1)(1:1) = "7"  )
                     OR (WRK-RECE130XX-SRYCD(IDY11 1)(1:3) = "059")
                       MOVE    1     TO   FLG-ETC
                       MOVE  IDY11   TO   IDY13
                     ELSE
                       IF WRK-RECE130XX-SRYCD(IDY11 1)(1:1) = "1" OR "6"
                         PERFORM 27011-SENTAKUCOM-HENSYU3-SEC
                         IF   FLG-GET = ZERO
                           MOVE    1     TO   FLG-ETC
                           MOVE  IDY11   TO   IDY13
                         END-IF
                       END-IF
                     END-IF
                   END-PERFORM
                   IF     FLG-GET  =   1
                     IF   WRK-RECE130XX-COMCDCHGFLG(IDX45 1) = "1"
                       MOVE   "810000001"
                                    TO   WRK-RECE130XX-SRYCD(IDX45 1)
                       MOVE    1    TO   FLG-SENTAKU-OKIKAE
                     END-IF
                     IF   FLG-ETC  =   1
                       MOVE    1         TO  FLG-SENTAKU-OKIKAE
                       MOVE    ZERO      TO  IDX46
                       MOVE    SPACE     TO  WRK-RECE130ZZ-REC
                       INITIALIZE            WRK-RECE130ZZ-REC
                       PERFORM  VARYING  IDY11  FROM  1  BY  1
                                 UNTIL   IDY11    >   IDY12
                         IF   IDY11 = IDY13
                           ADD   1   TO   IDX46
                           MOVE  WRK-RECE130XX-SRYACT-TBL(IDX45 1)
                                 TO   WRK-RECE130ZZ-SRYACT-TBL(IDX46 1)
                         END-IF
                         ADD     1   TO   IDX46
                         MOVE    WRK-RECE130XX-SRYACT-TBL(IDY11 1)
                                 TO   WRK-RECE130ZZ-SRYACT-TBL(IDX46 1)
                       END-PERFORM
                       PERFORM  VARYING  IDY11  FROM  1  BY  1
                                 UNTIL   IDY11    >   IDX45
                         MOVE   WRK-RECE130ZZ-SRYACT-TBL(IDY11 1)
                                 TO   WRK-RECE130XX-SRYACT-TBL(IDY11 1)
                       END-PERFORM
                     END-IF
                   ELSE
                     IF   WRK-RECE130XX-COMCDCHGFLG(IDX45 1) = "1"
                       MOVE "810000001" TO WRK-RECE130XX-SRYCD(IDX45 1)
                       MOVE  1          TO FLG-SENTAKU-OKIKAE
                     END-IF
                   END-IF
                 ELSE
                   IF     WRK-RECE130XX-COMCDCHGFLG(IDX45 1) = "1"
                       MOVE "810000001" TO WRK-RECE130XX-SRYCD(IDX45 1)
                       MOVE  1          TO FLG-SENTAKU-OKIKAE
                   END-IF
                 END-IF
               END-PERFORM
      *
               PERFORM 990-RECE1301-NEXT-SEC
      *
           END-PERFORM
      *
           .
       2701-SENTAKUCOM-HENSYU2-EXT.
           EXIT.
