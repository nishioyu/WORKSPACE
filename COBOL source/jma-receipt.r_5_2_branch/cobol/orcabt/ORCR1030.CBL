             ADD   1           TO IDX-RECE130
             MOVE  "810000001" TO WRK-RECE130XX-SRYCD(IDX-RECE130 1)
             MOVE  ZERO        TO WRK-RECE130XX-SRYSURYO(IDX-RECE130 1)
             MOVE  ZERO        TO WRK-RECE130XX-YUKOKETA(IDX-RECE130 1)
             MOVE  SPACE       TO WRK-RECE130XX-NAME(IDX-RECE130 1)
             MOVE  "1"         TO WRK-RECE130XX-RECEPTFLG(IDX-RECE130 1)
           END-PERFORM
