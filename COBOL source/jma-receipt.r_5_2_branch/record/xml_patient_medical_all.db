xml_patient_medical_all {
	context		int;
	object		object;
	mode		int;
	recordname	varchar(128);
	Patient_Medical_All {
#----->		患者情報
		Patient_Information {
		Patient_ID			varchar(20);
		WholeName			varchar(100);
		WholeName_inKana		varchar(100);
		BirthDate			varchar(10);
		Sex				varchar(1);
		};
#----->		診療日付
		Perform_Date			varchar(10);
		Department_Code			varchar(2);
		Department_Name			varchar(20);
		Sequential_Number		varchar(1);
#----->		伝票番号
		Invoice_Number			varchar(7);
#----->		保険組合せ番号
		Insurance_Combination_Number	varchar(4);
#----->		ドクター
		Physician_Code			varchar(5);
		Physician_WholeName		varchar(80);
#----->		更新日
		Update_Date			varchar(10);
		Update_Time			varchar(8);
                Medical_Information_Overflow    varchar(5);
#----->		診療内容
		Medical_Information  {
			Medical_Class			varchar(3);
			Medical_Class_Name		varchar(50);
			Medical_Class_Number		varchar(3);
			Medical_Class_Point		varchar(8);
			Medical_Class_Money 		varchar(8);
			Medical_Class_code 		varchar(1);
			Medical_Inclusion_Class		varchar(5);
#----->			包括検査の検査数
			Medical_Examination_Count	varchar(2);
			Medication_info   {
				Medication_Code		varchar(9);
				Medication_Name		varchar(140);
#----->                         コメント入力内容
				Medication_Name_Input_Value	varchar(200);
				Medication_Number	varchar(11);
				Unit_Code		varchar(3);
				Unit_Code_Name		varchar(24);
				Medication_Input_Info {
					Medication_Input_Code	varchar(10);
				} [5];
			} [50];
		} [50];
	} [2];
};
