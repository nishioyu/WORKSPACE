xml_onlinequaappreq2 {
	context		int;
	object		object;
	mode		int;
	recordname	varchar(128);
	onlinequaappreq2	{
		MessageHeader		{
#---   処理実行日時(YYYYMMDDHHmmss)
			ProcessExecutionTime		varchar(14);
#---   資格確認日
			QualificationConfirmationDate	varchar(08);
#---   医療機関コード
			MedicalInstitutionCode		varchar(10);
#---   任意のファイル識別子(医療機関固有項目)
			ArbitraryFileIdentifier		varchar(50);
#---   受付番号
			ReceptionNumber				varchar(38);
#---   処理結果区分 1:正常終了 2:処理中 9:異常終了
			SegmentOfResult				varchar(01);
#---   エラーコード
			ErrorCode					varchar(09);
#---   エラーメッセージ
			ErrorMessage				varchar(60);
#---   総処理件数
			NumberOfProcessingResult	varchar(04);
#---   処理件数(正常)
			NumberOfNormalProcessing	varchar(04);
#---   処理件数(エラー)
			NumberOfError				varchar(04);
#---   文字コード識別
			CharacterCodeIdentifier		varchar(01);
		};

#---   メッセージボディ部
		MessageBody   {
#---   個人単位情報
			BulkConfirmUnit   {
#---   資格確認照会用情報
				QualificationConfirmSearchInfo   {
#---   保険者番号
					InsurerNumber		varchar(08);
#---   被保険者証記号
					InsuredCardSymbol	varchar(80);
#---   被保険者証番号
					InsuredIdentificationNumber	varchar(80);
#---   被保険者証枝番
					InsuredBranchNumber	varchar(02);
#---   生年月日
					Birthdate		varchar(08);
#---   限度額適用認定証関連情報
					LimitApplicationCertificateRelatedinfo	{
#---   限度額適用認定証区分
					LimitApplicationCertificateClassification	varchar(02);
#---   限度額適用認定証適用区分
					LimitApplicationCertificateRelatedConsFlg	varchar(03);
					};
#---   任意の識別子(医療機関固有項目)
					ArbitraryIdentifier	varchar(50);
				};
#---   処理結果状況
				ProcessingResultStatus		varchar(01);
#---   処理結果コード
				ProcessingResultCode		varchar(09);
#---   処理結果メッセージ
				ProcessingResultMessage		varchar(60);
#---   資格有効性
				QualificationValidity		varchar(01);
#---   資格確認結果
				ResultOfQualificationConfirmation	{
#---   被保険者証区分
					InsuredCardClassification	varchar(02);
#---   保険者番号
					InsurerNumber				varchar(08);
#---   被保険者証記号
					InsuredCardSymbol			varchar(80);
#---   被保険者証番号
					InsuredIdentificationNumber	varchar(80);
#---   被保険者証枝番
					InsuredBranchNumber			varchar(02);
#---   本人・家族の別
					PersonalFamilyClassification	varchar(01);
#---   被保険者氏名(世帯主氏名)
					InsuredName					varchar(100);
#---   氏名
					Name						varchar(100);
#---   氏名(その他)
					NameOfOther					varchar(100);
#---   氏名カナ
					NameKana					varchar(100);
#---   氏名カナ(その他)
					NameOfOtherKana				varchar(100);
#---   性別1
					Sex1						varchar(01);
#---   性別2
					Sex2						varchar(01);
#---   生年月日
					Birthdate					varchar(08);
#---   住所
					Address						varchar(250);
#---   郵便番号	
					PostNumber					varchar(08);
#---   被保険者証交付年月日
					InsuredCertificateIssuanceDate	varchar(08);
#---   被保険者証有効開始年月日
					InsuredCardValidDate		varchar(08);
#---   被保険者証有効終了年月日
					InsuredCardExpirationDate	varchar(08);
#---   被保険者証一部負担金割合
					InsuredPartialContributionRatio	varchar(03);
#---   未就学区分
					PreschoolClassification		varchar(01);
#---   資格喪失事由
					ReasonOfLoss				varchar(02);
#---   保険者名称
					InsurerName					varchar(200);
#---   高齢受給者証情報
					ElderlyRecipientCertificateInfo	{
#---   高齢受給者証交付年月日
						ElderlyRecipientCertificateDate		varchar(08);
#---   高齢受給者証有効開始年月日
						ElderlyRecipientValidStartDate		varchar(08);
#---   高齢受給者証有効終了年月日
						ElderlyRecipientValidEndDate		varchar(08);
#---   高齢受給者証一部負担金割合
						ElderlyRecipientContributionRatio	varchar(03);
					} ;
#---   限度額適用認定証情報変更有無
					LimitApplicationCertificateChanged		varchar(01);
#---   照会番号
					ReferenceNumber				varchar(50);
				} [01] ;
			} [300];
		};
	};
};

