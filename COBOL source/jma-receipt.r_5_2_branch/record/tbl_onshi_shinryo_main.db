};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_onshi_shinryo_main_key_csr CURSOR FOR
		SELECT *
		FROM tbl_onshi_shinryo_main
			WHERE	HOSPNUM		=	:HOSPNUM
			AND	TBL_UUID	=	:TBL_UUID
			AND	PTID		=	:PTID
			AND	SRYYM		=	:SRYYM
		;
	};
};

path	all 	{
	DBSELECT	{
		DECLARE tbl_onshi_shinryo_main_all_csr CURSOR FOR
		SELECT *
		FROM tbl_onshi_shinryo_main
			WHERE   HOSPNUM    =   :HOSPNUM
 		;
	};
};

path	key2	{ 	
	DBSELECT	{
		DECLARE tbl_onshi_shinryo_main_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_onshi_shinryo_main
			WHERE	HOSPNUM	=	:HOSPNUM
			AND	PTID	=	:PTID
		ORDER	BY	SRYYM DESC
		;
	};
};

path	key3	{ 	
	DBSELECT	{
		DECLARE tbl_onshi_shinryo_main_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_onshi_shinryo_main
			WHERE	HOSPNUM	=	:HOSPNUM
			AND	PTID	=	:PTID
			AND	SRYYM	=	:SRYYM
		ORDER	BY	NYUGAIKBN,
				CHOZAI_SEQNUM,
				SHOHO_SEQNUM
		;
	};
};

path	key4	{ 	
