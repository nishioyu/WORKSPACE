tbl_youhou {
	CODE    		varchar(16);
	BASIC_C			varchar(1);
	BASIC_NAME      varchar(8);
	DETAIL_C        varchar(1);
	DETAIL_NAME     varchar(60);
	TIMING_C        number(1,0);
	TIMING_NAME     varchar(240);
	NAME            varchar(200);
	REFER_NO        number(4,0);
	YUKOSTYMD       varchar(8);
	YUKOEDYMD       varchar(8);
	CODE_C          number(1,0);
	ADD_CONDITION   number(1,0);
	ADD_TIMING      number(1,0);
	ADD_TIME        number(1,0);
	ADD_INTERVAL    number(1,0);
	ADD_BODY        number(1,0);
	KANA            varchar(480);
	CD_CHOZAI       number(3,0);
	TERMID   			varchar(16);
	OPID     			varchar(16);
	CREYMD   			varchar(8);
	CREHMS				varchar(6);
	UPYMD    			varchar(8);
	UPHMS				varchar(6);
#-----> 主キー更新用項目
	UPCODE		varchar(16),virtual;
	UPYUKOSTYMD	varchar(8),virtual;
	UPYUKOEDYMD	varchar(8),virtual;
};
primary	{
    CODE,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_youhou_key_csr CURSOR FOR
		SELECT *
		FROM tbl_youhou
		WHERE     CODE  =   :CODE
 			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_youhou_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_youhou
		WHERE    CODE  =   :CODE
            ORDER   BY  YUKOSTYMD
		;
	};
};
path	key3 	{
	DBSELECT	{
		DECLARE tbl_youhou_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_youhou
		WHERE    (NAME       LIKE :NAME
			OR    KANA       LIKE :KANA )
			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
            ORDER   BY  KANA
		;
	};
};

path	key4 	{
	DBSELECT	{
		DECLARE tbl_youhou_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_youhou
		WHERE     YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
            ORDER   BY  BASIC_C,
			            DETAIL_C,
                        CODE
		;
	};
};
path	key5 	{
	DBSELECT	{
		DECLARE tbl_youhou_key5_csr CURSOR FOR
		SELECT *
		FROM tbl_youhou
		WHERE    CODE  =   :CODE
            ORDER   BY  YUKOSTYMD  DESC
		;
	};
};
