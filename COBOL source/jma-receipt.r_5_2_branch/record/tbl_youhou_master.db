tbl_youhou_master {
	CODE    		varchar(16);
	BASIC_C			varchar(1);
	BASIC_NAME      varchar(8);
	DETAIL_C        varchar(1);
	DETAIL_NAME     varchar(60);
	TIMING_C        number(1,0);
	TIMING_NAME     varchar(240);
	NAME            varchar(200);
	REFER_NO        number(4,0);
	YUKOSTYMD       varchar(8);
	YUKOEDYMD       varchar(8);
	CODE_C          number(1,0);
	ADD_CONDITION   number(1,0);
	ADD_TIMING      number(1,0);
	ADD_TIME        number(1,0);
	ADD_INTERVAL    number(1,0);
	ADD_BODY        number(1,0);
	KANA            varchar(480);
	CD_CHOZAI       number(3,0);
	TERMID   			varchar(16);
	OPID     			varchar(16);
	CREYMD   			varchar(8);
	CREHMS				varchar(6);
	UPYMD    			varchar(8);
	UPHMS				varchar(6);
#-----> 主キー更新用項目
	UPCODE		varchar(16),virtual;
	UPYUKOSTYMD	varchar(8),virtual;
	UPYUKOEDYMD	varchar(8),virtual;
};
primary	{
    CODE,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_youhou_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_youhou
		WHERE     CODE  =   :CODE
 			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_youhou
			VALUES(
				:CODE,
				:BASIC_C,
				:BASIC_NAME,
				:DETAIL_C,
				:DETAIL_NAME,
				:TIMING_C,
				:TIMING_NAME,
				:NAME,
				:REFER_NO,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:CODE_C,
				:ADD_CONDITION,
				:ADD_TIMING,
				:ADD_TIME,
				:ADD_INTERVAL,
				:ADD_BODY,
				:KANA,
				:CD_CHOZAI,
				:TERMID,
				:OPID,
				:CREYMD,
				:CREHMS,
				:UPYMD,
				:UPHMS
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_youhou
			SET
				CODE          = :CODE,
				BASIC_C       = :BASIC_C,
				BASIC_NAME    = :BASIC_NAME,
				DETAIL_C      = :DETAIL_C,
				DETAIL_NAME   = :DETAIL_NAME,
				TIMING_C      = :TIMING_C,
				TIMING_NAME   = :TIMING_NAME,
				NAME          = :NAME,
				REFER_NO      = :REFER_NO,
				YUKOSTYMD     = :YUKOSTYMD,
				YUKOEDYMD     = :YUKOEDYMD,
				CODE_C        = :CODE_C,
				ADD_CONDITION = :ADD_CONDITION,
				ADD_TIMING    = :ADD_TIMING,
				ADD_TIME      = :ADD_TIME,
				ADD_INTERVAL  = :ADD_INTERVAL,
				ADD_BODY      = :ADD_BODY,
				KANA          = :KANA,
				CD_CHOZAI     = :CD_CHOZAI,
				TERMID        = :TERMID,
				OPID          = :OPID,
				CREYMD        = :CREYMD,
				CREHMS        = :CREHMS,
				UPYMD         = :UPYMD,
				UPHMS         = :UPHMS
 			WHERE   CODE          = :UPCODE
 			AND     YUKOSTYMD     = :UPYUKOSTYMD
 			AND     YUKOEDYMD     = :UPYUKOEDYMD
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_youhou
			WHERE   CODE       = :CODE
 			AND     YUKOSTYMD <= :YUKOSTYMD
			AND     YUKOEDYMD >= :YUKOEDYMD
		;
	};
};
